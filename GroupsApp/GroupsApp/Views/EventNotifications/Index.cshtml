@model IEnumerable<GroupsApp.Models.EventNotification>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>
@Html.ActionLink("Events", "Index", "Events")
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Message)
            </th>
            <th>
                Options
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Message)
            </td>
            <td>
                <a asp-action="Details" asp-route-id="@item.EventNotificationId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.EventNotificationId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>

